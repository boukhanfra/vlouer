{% extends '::base.html.twig' %}
{% block contenu %}

     <div class="container" style="padding:25px;">
         <div class="row">
            <div class="col-lg-6">
 {% image '@GsmLotIndexBundle/Resources/public/images/about1.jpg' %}
    <img src="{{ asset_url }}" alt="gsmlot phones exchange" style="width:500px;" />
{% endimage %} 
            </div>
            
            <div class="col-lg-4" style="text-align: justify">
    <blockquote>
  
{{ form_start(form) }}
<div class="form-group" >
			<label for="control-label" class="inputDefault">First Name</label>
			{{ form_widget(form.firstName,{ 'attr' :{ 'class' : 'form-control'} }) }}
</div>

<div class="form-group" >
			<label for="control-label" class="inputDefault">Last Name</label>
			{{ form_widget(form.lastName,{ 'attr' :{ 'class' : 'form-control'} }) }}
</div>
<div class="form-group" >
			<label for="control-label" class="inputDefault">Email</label>
			{{ form_widget(form.email,{ 'attr' :{ 'class' : 'form-control'} }) }}
</div>
<div class="form-group" >
			<label for="control-label" class="inputDefault">Subjet</label>
			{{ form_widget(form.objectMail,{ 'attr' :{ 'class' : 'form-control'} }) }}
</div>

	
	
	

</blockquote>
        
            </div>
     
     <div class="col-lg-2"></div>
     
     </div>
     
     </div>
     
{% endblock %}